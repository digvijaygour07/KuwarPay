{
  "version": 2,
  "builds": [
    {
      "src": "api.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
