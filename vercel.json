{
  "version": 2,
  "builds": [
    {
      "src": "netlify/functions/api/api.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/orders",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/orders/:id",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/payment-confirmation",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/feedback",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/success",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/failure",
      "dest": "netlify/functions/api/api.mjs"
    }
  ]
}