{
  "version": 2,
  "builds": [
    {
      "src": "/netlify/functions/api/api.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/public/styles.css",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/netlify/functions/api/api.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}