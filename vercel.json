{
  "version": 2,
  "builds": [
    {
      "src": "netlify/functions/api/api.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "netlify/functions/api/api.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "netlify/functions/api/api.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}